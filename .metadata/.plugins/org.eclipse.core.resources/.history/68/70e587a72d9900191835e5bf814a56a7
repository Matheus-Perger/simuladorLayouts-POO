package br.com.matheus.entities;

public class Calendario extends Display implements Formatter {

	private int dia;
	private int mes;
	private int ano;
	
	public Calendario(){
	}
	
	public Calendario(int dia, int mes, int ano) {
		this.dia = dia;
		this.mes = mes;
		this.ano = ano;
	}

	public int getDia() {
		return dia;
	}

	public void setDia(int dia) {
		this.dia = dia;
	}

	public int getMes() {
		return mes;
	}

	public void setMes(int mes) {
		this.mes = mes;
	}

	public int getAno() {
		return ano;
	}

	public void setAno(int ano) {
		this.ano = ano;
	}

	@Override
	public String formatter(T value) {
		// TODO Auto-generated method stub
		return null;
	}

	@Override
	public void show() {
		System.out.println(dia + "/" + mes + "/" + ano);
	}
}
